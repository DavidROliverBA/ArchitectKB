{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://vault.local/schemas/dailynote.schema.json",
  "title": "DailyNote Note",
  "description": "Schema for DailyNote note type in Obsidian vault",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "DailyNote",
      "description": "Note type identifier"
    },
    "date": {
      "type": "string",
      "examples": [
        "YYYY-MM-DD"
      ]
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": true
}