{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://vault.local/schemas/meeting.schema.json",
  "title": "Meeting Note",
  "description": "Schema for Meeting note type in Obsidian vault",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "Meeting",
      "description": "Note type identifier"
    },
    "date": {
      "type": "string",
      "examples": [
        "YYYY-MM-DD"
      ]
    },
    "project": {
      "type": [
        "string",
        "null"
      ]
    },
    "attendees": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^\\[\\[.+\\]\\]$"
      },
      "default": []
    },
    "summary": {
      "type": [
        "string",
        "null"
      ]
    },
    "collections": {
      "type": "string"
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": true
}