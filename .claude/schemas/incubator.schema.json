{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://vault.local/schemas/incubator.schema.json",
  "title": "Incubator Note",
  "description": "Schema for Incubator note type in Obsidian vault",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "Incubator",
      "description": "Note type identifier"
    },
    "status": {
      "type": "string",
      "enum": [
        "seed",
        "exploring",
        "validated",
        "accepted",
        "rejected"
      ]
    },
    "domain": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "outcome": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "required": [
    "type"
  ],
  "additionalProperties": true
}