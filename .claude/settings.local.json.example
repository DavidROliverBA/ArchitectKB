      "Bash(file *)",
      "Bash(mkdir *)",
      "Bash(touch *)",
      "Bash(cp *)",
      "Bash(mv *)",
      "Bash(rm *)",
      "Bash(chmod *)",
      "Bash(xargs *)",
      "Bash(comm *)",
      "Bash(diff *)",
      "Bash(tee *)",
      "Bash(bc *)",
      "Bash(for *)",
      "Bash(while *)",
      "Bash(if *)",
      "Bash(do *)",
      "Bash(done *)",
      "Bash(then *)",
      "Bash(fi *)",
      "Bash(sh *)",
      "Bash(bash *)",
      "Bash(export *)",
      "WebSearch",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:docs.github.com)",
      "Skill(*)"
    ],
    "defaultMode": "acceptEdits"
  },
  "hooks": {
    "PermissionRequest": [
      {
        "matcher": "Read|Glob|Grep",
        "hooks": [
          {
            "type": "command",
            "command": "echo '{\"hookSpecificOutput\": {\"hookEventName\": \"PermissionRequest\", \"decision\": {\"behavior\": \"allow\"}}}'"
          }
        ]
      }
    ]
  }
}
